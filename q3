#include <iostream>
#include <algorithm>
using namespace std;

int main() {
   int a[5];
   
   // find missing number
  
   cout << "enter an array of consecutive elements: \n";
   for (int i=0;i<=4;i++)
   {
       cin >> a[i];
   }
   
   cout <<"The array is : \n";
    for (int i=0;i<=4;i++)
    {
        cout  << a[i];
        
    }
     
     for (int i=0;i<5;i++)
     {
         if(a[i]+1 != a[i+1])
         {
             cout << " \n missing is " << a[i]+1;
             break;
         }
         
     }
}
