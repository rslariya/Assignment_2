#include <iostream>

using namespace std;

int main() {
   
   
   cout << "enter the lenght of array: ";
   int n;
   int count=0;
   cin >> n;
   int arr[n];
   for (int i=0;i<=n-1;i++)
   {
       cin>> arr[i];
       
   }
   cout << "the array is :\n";
    for (int i=0;i<=n-1;i++)
    {
        cout << arr[i];
    }
    
    for (int i=0;i<=n-1;i++)
    {
        if(arr[i]!=arr[i+1])
        {
            count++;
        }
    
    }
    cout << "\nnumber of distinct elemnts is " << count ;
    return 0;
}
