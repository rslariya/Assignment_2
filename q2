#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    //bubble sort
    int arr[7]={64,34,25,12,22,11,90};
    int n=7;
 for (int i=0;i<6;i++)
 {
    for (int j=0;j<7;j++)
    {
        if (arr[j]>arr[j+1])
        {
            swap (arr[j],arr[j+1]);
            
        }
    }
   
    
}
 
    for (int k=0;k<=6;k++)
    {
        cout << arr[k]<< "\t";
    }
}

