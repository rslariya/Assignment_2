#include <iostream>
#include <cctype>
using namespace std;

int main() {
    char ch;
    cout << "Enter an uppercase character: ";
    cin >> ch;

    if(isupper(ch)) {
        char lower = tolower(ch);
        cout << "Lowercase: " << lower << endl;
    } else {
        cout << "Character was not uppercase." << endl;
    }
    return 0;
}
